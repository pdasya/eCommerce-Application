{
  "name": "e-commerce-application",
  "version": "1.0.0",
  "description": "Final task for RSSchool",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve --open",
    "dev": "webpack --node-env=development",
    "prod": "webpack --node-env=production",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "ci:lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "ci:format": "prettier --check .",
    "stylelint": "stylelint './src/**/*{.css,.scss}' --fix",
    "ci:stylelint": "stylelint './src/**/*{.css,.scss}'",
    "prepare": "husky",
    "pre-push": "validate-branch-name && npm run test",
    "pre-commit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --ext .ts,.tsx --fix"
    ],
    "**/*{.css,.scss}": [
      "stylelint --fix"
    ],
    "*": [
      "prettier --write"
    ]
  },
  "validate-branch-name": {
    "pattern": "^(master|main|develop|sprint-\\d{1,2}){1}$|^(RSS-ECOMM-\\d{1,2}_\\d{2,4}|CUSTOM-ECOMM-\\d{1,4})-([a-z]{2,}([A-Z]([a-z]+)){0,})$",
    "errorMsg": "Invalid branch name."
  },
  "author": "ViP",
  "license": "ISC",
  "keywords": [],
  "dependencies": {
    "@commercetools/platform-sdk": "^7.8.0",
    "@commercetools/sdk-client-v2": "^2.5.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.17",
    "@mui/material": "^5.15.17",
    "@reduxjs/toolkit": "^2.2.3",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "formik": "^2.4.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.0",
    "react-toastify": "^10.0.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "5.1.1",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transformer-svg": "^2.0.2",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prettier-plugin-multiline-arrays": "^3.0.4",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "stylelint": "^16.5.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.3.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-plugin-css-modules": "^5.1.0",
    "validate-branch-name": "^1.3.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
